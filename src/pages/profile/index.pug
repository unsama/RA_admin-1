                            div.text-center(v-if='dataLoad3')
                                i.fa.fa-refresh.fa-spin.fa-3x.fa-fw
                            h3.text-center(style='margin: 15px 0;' v-if='!dataLoad3 && Object.keys(invoiceReqData).length === 0')
