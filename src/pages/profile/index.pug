                                                    i.fa.fa-eye
                                                button.btn.btn-danger.btn-sm.mar-bot-5(v-if='!cnicImgLoad && cnicImgURL === ""' disabled='disabled')
                                                    i.fa.fa-exclamation
                                                button.btn.btn-info.btn-sm.mar-bot-5(v-if='cnicImgLoad' disabled='disabled')
                                                    i.fa.fa-refresh.fa-spin.fa-fw
                                        .col-md-12
                                            .col-xs-6
