                                                button.btn.btn-danger.btn-sm.mar-bot-5(v-if='!letterImgLoad && letterImgURL === ""' disabled='disabled')
                                                    i.fa.fa-exclamation
                                                button.btn.btn-info.btn-sm.mar-bot-5(v-if='letterImgLoad' disabled='disabled')
